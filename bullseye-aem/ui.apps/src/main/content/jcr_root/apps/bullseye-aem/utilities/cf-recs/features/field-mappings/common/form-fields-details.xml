<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/container">
    <items jcr:primaryType="nt:unstructured">
        <cfModelPathDisabledSelect
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="CF Model"
                fieldDescription="The name of the Content Fragment Model."
                disabled="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-items"
                    variant="default"/>
        </cfModelPathDisabledSelect>
        <cfModelPath
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                name="./cfModelPath"
                value="${param.selectedCfModelPath}"/>
        <parentCfPaths
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                name="./parentCfPaths"
                multiple="{Boolean}true"
                filter="folder"
                rootPath="/content/dam"
                fieldDescription="The parent paths of the Content Fragments to export."
                fieldLabel="Parent CF Paths">
        </parentCfPaths>
        <id
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="id"
                name="./idCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                variant="default"/>
        </id>
        <name
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="name"
                name="./nameCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                variant="default"/>
        </name>
        <message
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="message"
                name="./messageCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                variant="default"/>
        </message>
        <category
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="category"
                name="./categoriesCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </category>
        <brand
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="brand"
                name="./brandCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </brand>
        <inventory
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="inventory"
                name="./inventoryCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </inventory>
        <margin
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="margin"
                name="./marginCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </margin>
        <pageUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="pageUrl"
                name="./pageUrlCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </pageUrl>
        <thumbnailUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="thumbnailUrl"
                name="./thumbnailUrlCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </thumbnailUrl>
        <value
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldLabel="value"
                name="./valueCfFieldName"
                emptyOption="{Boolean}true"
                requried="{Boolean}true">
            <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                    variant="default"/>
        </value>
        <fieldMappings
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                fieldDescription="Map Target Recommendation attributes."
                fieldLabel="Custom Field Mappings"
                composite="{Boolean}true">
            <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container"
                    name="./custom-mappings">
                <items jcr:primaryType="nt:unstructured">
                    <pair jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/foundation/section">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Target Field Name"
                                    name="targetFieldName"/>
                            <value
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="CF Field Name"
                                    name="cfFieldName"
                                    emptyOption="{Boolean}true">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="bullseye-aem/components/content/cf-recs/field-mapping/cf-model-field-items"
                                        variant="default"/>
                            </value>
                        </items>
                        <layout jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                    </pair>
                </items>
            </field>
        </fieldMappings>
        <enabled
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                text="Enabled"
                name="./enabled"
                value="{Boolean}true"
                uncheckedValue="{Boolean}false"
                checked="{Boolean}true"/>
    </items>
</jcr:root>
